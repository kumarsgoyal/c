// this piece of code find whether a number is power of 2 or not.

#include <stdio.h>


int ispower(int num)
{
    return (num & (num - 1));
    // if return 0 mean number is power of 2
    // else number is not power of 2
}


int main() {
	int num = 0;
	scanf("%d", &num);
	int flag = ispower(num);
	if(flag == 0)
	{
	    printf("Number is power of 2");
	}
	else
	{
	    printf("Number is not power of 2");
	}
	return 0;
}
